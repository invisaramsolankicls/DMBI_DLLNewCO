    <validationRules>
        <fullName>Check_Reject_AND_Clear</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Cleared__c , Rejected__c )</errorConditionFormula>
        <errorMessage>You can either clear or reject a payment, but not do both.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_State_Check</fullName>
        <active>true</active>
        <errorConditionFormula>Contract__r.Invalid_Data__c</errorConditionFormula>
        <errorMessage>Lease is in invalid state via an unexpected event. Please contact Cloud Lending support before posting this payment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lease_Status_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp;
(TEXT(Contract__r.Lease_Status__c) = &apos;Partial Application&apos; ||
TEXT(Contract__r.Lease_Status__c) = &apos;Pending Approval&apos; ||
TEXT(Contract__r.Lease_Status__c) = &apos;Canceled&apos; ||
TEXT(Contract__r.Lease_Status__c) = &apos;Approved&apos;)</errorConditionFormula>
        <errorMessage>Invalid Lease Status. You cannot record a repayment transaction for this lease.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transaction_Date_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Transaction_Date__c &gt;  $User.Current_Branch_s_System_Date__c  ,  NOT(Skip_Validation__c), ISNEW())</errorConditionFormula>
        <errorMessage>Transaction/Receipt date cannot be greater than system date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Zero_Txn_Amount_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ROUND(Transaction_Amount__c,2) == 0</errorConditionFormula>
        <errorMessage>Transaction amount should be greater than zero.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Apply_Payment</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Apply Payment</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/13.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)}
var namespacePrefix = &quot;{!$Setup.Org_Parameters__c.Namespace_Prefix__c}&quot;;
if (namespacePrefix != null &amp;&amp; namespacePrefix != &quot;&quot;)
namespacePrefix += &quot;/&quot;;
var className = namespacePrefix + &quot;BulkLeasePaymentAction&quot;;
var parameter1 = &quot;{!Lease_Payment_Transaction__c.Id}&quot;;
var retVal = sforce.apex.execute(className,&quot;clearPaymentTransaction&quot;, {paymentTransactionId:parameter1 });
window.alert(retVal);
window.location.reload();</url>
    </webLinks>
