        <errorDisplayField>RecurrenceMonthOfYear__c</errorDisplayField>
        <errorMessage>Recurrence Month of Year cannot be selected if the holiday is not marked as Recurring</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Recurrence_Type_if_not_recurring</fullName>
        <active>true</active>
        <description>Recurrence Type cannot be selected if the holiday is not marked as Recurring</description>
        <errorConditionFormula>AND(NOT(IsRecurrence__c), NOT(ISBLANK(TEXT(RecurrenceType__c))))</errorConditionFormula>
        <errorDisplayField>RecurrenceType__c</errorDisplayField>
        <errorMessage>Recurrence Type cannot be selected if the holiday is not marked as Recurring</errorMessage>
    </validationRules>
    <validationRules>
