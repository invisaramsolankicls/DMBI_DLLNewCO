        <active>true</active>
        <description>Recurrence End Date cannot be set if the holiday is not marked as Recurring</description>
        <errorConditionFormula>AND(NOT(IsRecurrence__c), NOT(ISBLANK(RecurrenceEndDateOnly__c)))</errorConditionFormula>
        <errorDisplayField>RecurrenceEndDateOnly__c</errorDisplayField>
        <errorMessage>Recurrence End Date cannot be set if the holiday is not marked as Recurring</errorMessage>
    </validationRules>
    <validationRules>
