    <validationRules>
        <fullName>Activity_Date_if_recurring_or_one_day</fullName>
        <active>true</active>
        <description>This date should not be populated if the holiday is marked recurring.
Also, this date should be populated if the holiday is not marked recurring.</description>
        <errorConditionFormula>OR(AND(IsRecurrence__c, NOT(ISBLANK(ActivityDate__c))), AND(NOT(IsRecurrence__c), ISBLANK(ActivityDate__c)))</errorConditionFormula>
        <errorDisplayField>ActivityDate__c</errorDisplayField>
        <errorMessage>One of the following :
1. Activity date MUST NOT be set if the holiday is marked RECURRING.
2. Activity date MUST be set if the holiday is marked NOT RECURRING.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EndTime_not_less_or_equal_to_StartTime</fullName>
        <active>true</active>
        <description>The End Time for the Holiday cannot be less than or equal to the Start Time of the Holiday.</description>
        <errorConditionFormula>TIMEVALUE(EndTimeInMinutes__c) &lt;=  TIMEVALUE(StartTimeInMinutes__c)</errorConditionFormula>
        <errorDisplayField>EndTimeInMinutes__c</errorDisplayField>
        <errorMessage>Holiday end time should occur after the start time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_cannot_be_less_than_Start_Date</fullName>
        <active>true</active>
        <description>The End Date for the Holiday should be greater than or equal to the Start Date of the Holiday.</description>
        <errorConditionFormula>RecurrenceEndDateOnly__c &lt; RecurrenceStartDate__c</errorConditionFormula>
        <errorDisplayField>RecurrenceEndDateOnly__c</errorDisplayField>
        <errorMessage>Holiday end date should be greater than or equal to the start date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Is_DayOfMonth_valid_for_MonthOfYear</fullName>
        <active>true</active>
        <description>This is a validation rule that checks if the given Recurring Day of Month is valid for the chosen Recurring Month of Year.</description>
        <errorConditionFormula>OR(AND(OR(ISPICKVAL(RecurrenceMonthOfYear__c, &quot;February&quot;),ISPICKVAL(RecurrenceMonthOfYear__c, &quot;April&quot;),ISPICKVAL(RecurrenceMonthOfYear__c, &quot;June&quot;),ISPICKVAL(RecurrenceMonthOfYear__c, &quot;September&quot;),ISPICKVAL(RecurrenceMonthOfYear__c, &quot;November&quot;)),RecurrenceDayOfMonth__c = 31),AND(ISPICKVAL(RecurrenceMonthOfYear__c, &quot;February&quot;),RecurrenceDayOfMonth__c = 30))</errorConditionFormula>
        <errorDisplayField>RecurrenceDayOfMonth__c</errorDisplayField>
        <errorMessage>Recurring Day of Month is not a valid day for the chosen Recurring Month of Year</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_End_Time_if_all_day</fullName>
        <active>true</active>
        <description>This validation rule is applied if a holiday is marked as an all day event and then tries to set the end time as well.</description>
        <errorConditionFormula>AND(AND(IsAllDay__c, NOT(ISBLANK(EndTimeInMinutes__c))),NOT(IsRecurrence__c))</errorConditionFormula>
        <errorDisplayField>EndTimeInMinutes__c</errorDisplayField>
        <errorMessage>End time cannot be defined if the holiday is an all day event.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Recurrence_Day_Mask_if_not_recurring</fullName>
        <active>true</active>
        <description>Recurrence day of week mask cannot be set if the holiday is not marked as Recurring</description>
        <errorConditionFormula>AND(NOT(IsRecurrence__c), NOT(ISBLANK(RecurrenceDayOfWeekMask__c)))</errorConditionFormula>
        <errorDisplayField>RecurrenceDayOfWeekMask__c</errorDisplayField>
        <errorMessage>Recurrence day of week mask cannot be set if the holiday is not marked as Recurring</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Recurrence_Day_if_not_recurring</fullName>
        <active>true</active>
        <description>Recurrence Day of Month cannot be set if the holiday is not marked as Recurring</description>
        <errorConditionFormula>AND(NOT(IsRecurrence__c), NOT(ISBLANK(RecurrenceDayOfMonth__c)))</errorConditionFormula>
        <errorDisplayField>RecurrenceDayOfMonth__c</errorDisplayField>
        <errorMessage>Recurrence Day of Month cannot be set if the holiday is not marked as Recurring</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Recurrence_Instance_if_not_recurring</fullName>
        <active>true</active>
        <description>Recurrence Instance cannot be selected if the holiday is not marked as Recurring</description>
        <errorConditionFormula>AND(NOT(IsRecurrence__c), NOT(ISBLANK(TEXT(RecurrenceInstance__c))))</errorConditionFormula>
        <errorDisplayField>RecurrenceInstance__c</errorDisplayField>
        <errorMessage>Recurrence Instance cannot be selected if the holiday is not marked as Recurring</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Recurrence_Interval_if_not_recurring</fullName>
        <active>true</active>
        <description>Recurrence Interval cannot be provided if the holiday is not marked as Recurring</description>
        <errorConditionFormula>AND(NOT(IsRecurrence__c), NOT(ISBLANK(RecurrenceInterval__c )))</errorConditionFormula>
        <errorDisplayField>RecurrenceInterval__c</errorDisplayField>
        <errorMessage>Recurrence Interval cannot be provided if the holiday is not marked as Recurring</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Recurrence_Month_if_not_recurring</fullName>
