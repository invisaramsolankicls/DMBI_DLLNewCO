        <description>Due Day cannot be greater than 31, nor less than 1.</description>
        <errorConditionFormula>Due_Day__c &gt;31 || Due_Day__c &lt;1</errorConditionFormula>
        <errorMessage>Due Day cannot be greater than 31, nor less than 1.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>First_Payment_Date_Validation</fullName>
        <active>true</active>
        <description>Expected First Payment Date should be greater than or equal to the Start Date.</description>
        <errorConditionFormula>Expected_First_Payment_Date__c &lt;  Expected_Start_Date__c</errorConditionFormula>
        <errorMessage>Expected First Payment Date should be greater than or equal to the Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Funding_in_Tranches</fullName>
        <active>true</active>
        <description>Check that Draw Period End date is not null when
Funding in Tranches is Checked</description>
        <errorConditionFormula>AND( Funding_in_Tranches__c = true, ISNULL(Draw_Period_End_Date__c))</errorConditionFormula>
        <errorMessage>Draw Period End Date cannot be empty for Funding in Tranches.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Payment_Frequency_Multiplier_Validation</fullName>
        <active>true</active>
        <description>Payment Frequency Multiplier should not be less than or equal to zero .
Payment Frequency Multiplier should not be allowed to input for Payment Frequency other than Monthly, Weekly and Daily</description>
        <errorConditionFormula>IF( ISBLANK(Payment_Frequency_Multiplier__c),FALSE,
 OR(Payment_Frequency_Multiplier__c &lt;= 0,
  NOT(
   OR( ISPICKVAL(Payment_Frequency__c,&apos;Daily&apos;),
      ISPICKVAL(Payment_Frequency__c,&apos;WEEKLY&apos;),
      ISPICKVAL(Payment_Frequency__c,&apos;MONTHLY&apos;)
     )
    )
  )
)</errorConditionFormula>
        <errorMessage>Either value is less than or equal to zero, or kindly choose correct value for Payment Frequency(Daily, Weekly or Monthly)</errorMessage>
    </validationRules>
