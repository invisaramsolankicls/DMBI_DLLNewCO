    <validationRules>
        <fullName>Amortization_Method_Not_Null</fullName>
        <active>true</active>
        <description>Check if the amortization method is not null if  fee is amortized.</description>
        <errorConditionFormula>Amortize__c == true  &amp;&amp; (TEXT(Amortization_Method__c) == null || TEXT(Amortization_Period__c) == null)</errorConditionFormula>
        <errorMessage>Amortization Method / Amortization period cannot be null</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amount_limits_Check</fullName>
        <active>true</active>
        <description>Entered Amount should be between maximum and minimum amounts entered.</description>
        <errorConditionFormula>if(AND(Minimum_Amount__c&lt;=Amount__c, Amount__c&lt;=Maximum_Amount__c), false, true) || Amount__c &lt; 0</errorConditionFormula>
        <errorMessage>Amount entered cannot be negative and should be between Maximum and Minimum amounts entered. Maximum and Minimum Amount default value 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Max_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>Maximum_Amount__c &lt; 0</errorConditionFormula>
        <errorMessage>Maximum Amount cannot be negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Min_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>Minimum_Amount__c &lt; 0</errorConditionFormula>
        <errorMessage>Minimum Amount cannot be negative</errorMessage>
    </validationRules>
