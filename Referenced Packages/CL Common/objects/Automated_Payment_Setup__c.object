    <validationRules>
        <fullName>ACH_Start_Date_greater_than_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Type__c,&apos;RECURRING&apos;) , Recurring_ACH_End_Date__c  &lt; Recurring_ACH_Start_Date__c )</errorConditionFormula>
        <errorMessage>ACH End Date should be greater than ACH Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Retry_Configuration</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Retry__c , &apos;Enabled&apos;),  OR(ISBLANK(Number_of_Retry_Attempts__c),  ISNULL(Number_of_Retry_Attempts__c), ISBLANK(Retry_Attempt_Interval__c),  ISNULL(Retry_Attempt_Interval__c )))</errorConditionFormula>
        <errorMessage>Please provide Number of Retry Attempts and/or Retry Attempt Interval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Debit_Day_must_not_be_null</fullName>
        <active>true</active>
        <errorConditionFormula>Debit_Day__c == NULL
&amp;&amp;
ISPICKVAL(Type__c, &apos;RECURRING&apos;)</errorConditionFormula>
        <errorMessage>Debit Day must not be null.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Frequency_Cycle_Input_Validation</fullName>
        <active>true</active>
        <description>Frequency Cycle can not be less than or equal to zero
Frequency Cycle is allowed only for Monthly, Daily and Weekly Frequencies</description>
        <errorConditionFormula>AND( NOT( ISNULL(Frequency_Cycle__c) ) ,OR(Frequency_Cycle__c &lt;= 0 , AND( Frequency_Cycle__c &gt; 1,OR(ISPICKVAL(Frequency__c, &apos;Bi-weekly&apos;),
ISPICKVAL(Frequency__c, &apos;Semi-Monthly&apos;),
ISPICKVAL(Frequency__c, &apos;Bi-Monthly&apos;),
ISPICKVAL(Frequency__c, &apos;Quarterly&apos;),
ISPICKVAL(Frequency__c, &apos;Semi-Annual&apos;),
ISPICKVAL(Frequency__c, &apos;Annual&apos;),
ISPICKVAL(Frequency__c, &apos;Single-Payment&apos;)))))</errorConditionFormula>
        <errorDisplayField>Frequency_Cycle__c</errorDisplayField>
        <errorMessage>Kindly Add correct Value for Frequency Cycle , Either value is less than or equal to zero, or selected Payment Frequency is not supported.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Frequency_check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Type__c,&apos;RECURRING&apos;),OR(ISNULL(TEXT(Frequency__c)),ISBLANK(TEXT(Frequency__c))))</errorConditionFormula>
        <errorDisplayField>Frequency__c</errorDisplayField>
        <errorMessage>Frequency can not be null for &apos;Recurring&apos; type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Type_cannot_be_null</fullName>
        <active>true</active>
        <description>Type should always be present</description>
        <errorConditionFormula>OR(ISBLANK(TEXT(Type__c)), ISNULL(TEXT(Type__c)))</errorConditionFormula>
        <errorMessage>Type must not be null.</errorMessage>
    </validationRules>
</CustomObject>
