    <validationRules>
        <fullName>Check_Default_Term</fullName>
        <active>true</active>
        <errorConditionFormula>Default_Number_of_Installments__c &lt;= 0</errorConditionFormula>
        <errorMessage>The Default Term must be greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Default_With_Max_Term</fullName>
        <active>true</active>
        <errorConditionFormula>Default_Number_of_Installments__c &gt;  Max_Number_of_Installments__c</errorConditionFormula>
        <errorMessage>The default term must be lesser than or equal to the maximum term.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Default_With_Min_Term</fullName>
        <active>true</active>
        <errorConditionFormula>Default_Number_of_Installments__c &lt;  Min_Number_of_Installments__c</errorConditionFormula>
        <errorMessage>The default term must be greater than or equal to the minimum term.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Delinquency_Grace_Days</fullName>
        <active>true</active>
        <errorConditionFormula>Delinquency_Grace_Days__c &lt; 0</errorConditionFormula>
        <errorMessage>The delinquency grace days must be greater than or equal to zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Evergreen_Term</fullName>
        <active>true</active>
        <errorConditionFormula>Evergreen_Term__c &lt; 0</errorConditionFormula>
        <errorMessage>The Evergreen Term must be greater than or equal to zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Late_Charge_Grace_Days</fullName>
        <active>true</active>
        <errorConditionFormula>Late_Charge_Grace_Days__c &lt; 0</errorConditionFormula>
        <errorMessage>The Late Charge Grace Days must be greater than or equal to zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Max_Financed_Amt</fullName>
        <active>true</active>
        <errorConditionFormula>Max_Lease_Amount__c &lt;= 0</errorConditionFormula>
        <errorMessage>The maximum financed amount must be greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Max_Term</fullName>
        <active>true</active>
        <errorConditionFormula>Max_Number_of_Installments__c &lt;= 0</errorConditionFormula>
        <errorMessage>The maximum contract term must be greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Min_Financed_Amt</fullName>
        <active>true</active>
        <errorConditionFormula>Min_Lease_Amount__c &lt;= 0</errorConditionFormula>
        <errorMessage>The minimum financed amount must be greater than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Min_Max_Financed_Amt</fullName>
        <active>true</active>
        <errorConditionFormula>Min_Lease_Amount__c &gt; Max_Lease_Amount__c</errorConditionFormula>
        <errorMessage>The minimum financed amount must be less than the maximum financed amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Min_Max_Term</fullName>
        <active>true</active>
        <errorConditionFormula>Min_Number_of_Installments__c &gt; Max_Number_of_Installments__c</errorConditionFormula>
        <errorMessage>The minimum contract term must be less than the maximum contract term.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Min_Term</fullName>
        <active>true</active>
        <description>Min term should be less than Max term</description>
        <errorConditionFormula>Min_Number_of_Installments__c &lt;= 0</errorConditionFormula>
        <errorMessage>The minimum contract term must be greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Pre_Bill_Days</fullName>
        <active>true</active>
        <errorConditionFormula>Pre_Bill_Days__c &lt; 0</errorConditionFormula>
        <errorMessage>The Pre Bill Days must be greater than or equal to zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Product_Name</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Name) || ISNULL(Name)</errorConditionFormula>
        <errorMessage>Lease Product name cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Product_Type</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Lease_Product_Type__c,&quot;&quot;)</errorConditionFormula>
        <errorMessage>Product Type Cannot be Null</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Termination_Lead_Days</fullName>
        <active>true</active>
        <errorConditionFormula>Termination_Lead_Days__c &lt; 0</errorConditionFormula>
        <errorMessage>The Termination Lead Days must be greater than or equal to zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Unique_Lending_Product</fullName>
        <active>true</active>
        <errorConditionFormula>Name = VLOOKUP( $ObjectType.Lease_Product__c.Fields.Name , $ObjectType.Lease_Product__c.Fields.Name , Name )
&amp;&amp;
Id &lt;&gt; VLOOKUP( $ObjectType.Lease_Product__c.Fields.Id , $ObjectType.Lease_Product__c.Fields.Name , Name )</errorConditionFormula>
        <errorMessage>Product Name already exists.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Operating_Lease_Subvention_Stream_Basis</fullName>
        <active>true</active>
        <description>Income Basis not valid for Operating Lease</description>
        <errorConditionFormula>IF(AND(ISPICKVAL(Product_Sub_Type__c, &quot;OPERATING LEASE&quot;), ISPICKVAL(Subvention_Stream_Basis__c, &quot;Income Basis&quot;)), true, false)</errorConditionFormula>
        <errorMessage>Subvention Stream Basis&apos;s Income Basis not valid for Operating Leases.</errorMessage>
    </validationRules>
