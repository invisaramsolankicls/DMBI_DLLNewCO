    <validationRules>
        <fullName>ACH_Fields_Check</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ACH_Active__c ,
OR(ISBLANK( ACH_Routing_Number__c ),ISBLANK( ACH_Account_Number__c ), ISBLANK( ACH_Debit_Amount__c ), ISBLANK( ACH_Next_Debit_Date__c ))
)</errorConditionFormula>
        <errorMessage>One of the required ACH fields is missing:
Routing number, Account number, Debit amount or Next debit date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Product_Type</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(Product_Type__c,&quot;&quot;)</errorConditionFormula>
        <errorMessage>Product Type Cannot be Null</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_Date_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>Closed_Date__c  &gt;  TODAY()</errorConditionFormula>
        <errorMessage>Close Date cant not be greater than System date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Currency_Change_check</fullName>
        <active>true</active>
        <description>Currency code should not change once contract created with one currency.</description>
        <errorConditionFormula>if( Currency_Code__c ==null, false ,  ISCHANGED(Currency_Code__c) )</errorConditionFormula>
        <errorMessage>Currency for contract cannot be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dealer_Payment_Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>Dealer_Payment_Date__c &gt; Maturity_Date__c  ||  Dealer_Payment_Date__c   &lt; Contract_Date__c</errorConditionFormula>
        <errorDisplayField>Dealer_Payment_Date__c</errorDisplayField>
        <errorMessage>Dealer Payment Date cannot be beyond Contract Maturity Date or before Contract Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Excess_Negative_check</fullName>
        <active>false</active>
        <description>Excess has to be put as positive.</description>
        <errorConditionFormula>Excess__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Excess__c</errorDisplayField>
        <errorMessage>Excess Cannot Be Negative.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expected_Disbursal_Date_check</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
 ISCHANGED(Contract_Date__c), Contract_Date__c &lt;   Branch__r.Current_System_Date__c
)</errorConditionFormula>
        <errorMessage>Expected Disbursal Date cannot be less than current system date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>First_Payment_Date_Validation</fullName>
        <active>true</active>
        <description>First Payment Date can not be prior to the Contract Date.
OR
First Payment Date should be equal or greater than Contract Date.</description>
        <errorConditionFormula>First_Payment_Date__c  &lt;  Contract_Date__c</errorConditionFormula>
        <errorMessage>First Payment Date cannot be prior to the Contract Date!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Recurring_OT_ACH_Conflict_Check</fullName>
        <active>false</active>
        <errorConditionFormula>NOT( ISBLANK( ACH_Next_Debit_Date__c ))  &amp;&amp;  NOT( ISBLANK( OT_ACH_Debit_Date__c )) &amp;&amp; (ACH_Next_Debit_Date__c == OT_ACH_Debit_Date__c )</errorConditionFormula>
        <errorMessage>System Limitation - You cannot set up a One Time ACH and Recurring ACH transaction on the same day.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Term_Residual_Months_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Term_Residual_Months__c !=0,    OR(ISBLANK(TEXT(Residual_Type__c)),ISPICKVAL( Residual_Type__c, &quot;FMV&quot;)))</errorConditionFormula>
        <errorMessage>Term Residual Months only valid for Residual type: TERM RESIDUAL. Not valid for either Residual type is not mentioned or FMV</errorMessage>
    </validationRules>
